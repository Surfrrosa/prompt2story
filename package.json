{
  "name": "prompt2story",
  "version": "1.0.0",
  "description": "AI-powered user story generator",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "vercel dev",
    "build": "npm --prefix frontend run build",
    "typecheck": "tsc -p tsconfig.json --noEmit || true",
    "lint": "eslint . || true",
    "format": "prettier --write .",
    "test": "vitest run || true",
    "check-cruft": "node ./scripts/check-cruft.mjs || echo 'ℹ️  (stub) No cruft check implemented yet.'"
  },
  "keywords": [
    "ai",
    "user-stories",
    "openai",
    "vercel",
    "react"
  ],
  "author": "Surfrrosa",
  "license": "MIT",
  "dependencies": {
    "@vercel/node": "^5.3.22",
    "formidable": "^3.5.4",
    "openai": "^4.55.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "typescript": "^5.6.2"
  },
  "overrides": {
    "path-to-regexp": "^8.0.0",
    "undici": "^6.0.0"
  }
}
